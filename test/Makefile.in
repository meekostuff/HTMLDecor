srcdir = @srcdir@
VPATH = @srcdir@

include @top_srcdir@/mk/gnu.bsdvars.mk

SUBDIRS = html

.SUFFIXES :
.SUFFIXES : .html .xhtml .js .css

# Rules
all : build

build :
	for dir in ${SUBDIRS}; do make -C $${dir} build; done

clean :
	for dir in ${SUBDIRS}; do make -C $${dir} clean; done

install : 
	for dir in ${SUBDIRS}; do make -C $${dir} install; done
